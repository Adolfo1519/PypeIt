"""
This script runs PypeIt on DEIMOS in quick look mode
"""

from pypeit.scripts import ql_keck_deimos
import sys

if __name__ == '__main__':
    args = ql_keck_deimos.parser()
    rtval = ql_keck_deimos.main(args)
    sys.exit(rtval)

