"""
This script runs PypeIt on DEIMOS in quick look mode
"""

from pypeit.scripts import deimos_quicklook
import sys

if __name__ == '__main__':
    args = deimos_quicklook.parser()
    rtval = deimos_quicklook.main(args)
    sys.exit(rtval)

